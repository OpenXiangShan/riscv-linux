/dts-v1/;

#include "nanhu-v3.dtsi"

/ {
	#address-cells = <2>;
	#size-cells = <2>;
	compatible = "bosc,nanhu-v3-dev", "bosc,nanhu-v3";
	model = "bosc,nanhu-v3";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		bootargs = "root=/dev/nvme0n1p2 rootfstype=ext4 rw rootwait console=ttyS0 earlycon noinitrd";
		stdout-path = "serial0:9600n8";
	};
};

&cpu0 {
	status = "okay";
};

&cpu1 {
	status = "okay";
};

&cpu2 {
	status = "okay";
};

&cpu3 {
	status = "okay";
};
